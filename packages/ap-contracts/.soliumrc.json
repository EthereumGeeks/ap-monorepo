{
  "extends": "solium:all",
  "plugins": [
    "security"
  ],
  "rules": {
    "quotes": ["error", "double"],
    "indentation": ["error", 4],
    "linebreak-style": "off",
    "no-empty-blocks": "off",
    "whitespace": "off",
    "operator-whitespace": "off",
    "lbrace": "off",
    "no-experimental": "off",
    "mixedcase": "off",
    "security/no-block-members": "off"
  }
}