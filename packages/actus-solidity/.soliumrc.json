{
  "extends": "solium:recommended",
  "plugins": [
    "security"
  ],
  "rules": {
    "quotes": ["error", "double"],
    "indentation": "off",
    "linebreak-style": "off",
    "no-empty-blocks": "off",
    "operator-whitespace": "off",
    "lbrace": "off",
    "no-experimental": "off",
    "mixedcase": "off",
    "arg-overflow": "off"
  }
}
